int residual_phi_matome()
{


  TCanvas *cc = new TCanvas("cc","cc");
  cc -> Divide(3,3);

  ///nocut
  cc -> cd (1);

  TH1D *hist_n = new TH1D("hist_n","title",40,-10,10);
  tree -> Draw("(1/muonPt-1/(abs(L2SAPt*1000)))/(1/muonPt) >> hist_n");
  hist_n -> SetTitle("muonPt residual;muon residual;Number of events");

  gPad -> Update();

  TPaveStats *stn = (TPaveStats*) hist_n -> FindObject("stats");
  stn -> SetX1NDC(0.7);
  stn -> SetX2NDC(0.9);
  stn -> SetY1NDC(0.8);
  stn -> SetY2NDC(1.0);

  hist_n -> Draw();

  /*

  for (int i = 1; i < 9 ; i = i+1){
