    hist_i -> SetTitle("(i);muon residual;Number of events");

    gPad -> Update();

    TPaveStats *sti = (TPaveStats*) hist_i -> FindObject("stats");
    sti -> SetX1NDC(0.7);
    sti -> SetX2NDC(0.9);
    sti -> SetY1NDC(0.8);
    sti -> SetY2NDC(1.0);

