int DimuonMass()
{
  TH1D *histgram = new TH1D("histgram","title",20,0,10);
  tree->Draw("dimuon_mass/1000>>histgram");
  histgram->SetTitle("invariant mass of dimuon;invariant mass (GeV);Number of events");

  gPad->Update();

  TPaveStats *st1 = (TPaveStats*) histgram->FindObject("stats");
  st1->SetX1NDC(0.7);
  st1->SetX2NDC(0.9);
  st1->SetY1NDC(0.8);
  st1->SetY2NDC(1.0);

  histgram->Draw();

  return 0;

}
