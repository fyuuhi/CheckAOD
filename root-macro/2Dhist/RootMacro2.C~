int RootMacro2()
{
  TH2D *muon= new TH2D("muon","title",50,0,1200,50,-8,2);
  tree->Draw("(1/muonPt-1/(abs(L2SAPt*1000)))/(1/muonPt):(muonPt/1000)>>muon","abs(muon_eta)>1");
  muon->SetTitle("muonPt residual (|eta|>1) ;muonPt (GeV);muonPt residual");

  gPad->Update();

  TPaveStats *st1 = (TPaveStats*) muon->FindObject("stats");
  st1->SetX1NDC(0.7);
  st1->SetX2NDC(0.9);
  st1->SetY1NDC(0.8);
  st1->SetY2NDC(1.0);

  muon->Draw("COLZ");

  return 0;

}
